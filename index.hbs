{{!< default}}

<ul class="block-list post-list">

{{#foreach posts}}

<li class="cf {{post_class}}">
    <div class="post-meta">{{date format="Do MMMM YYYY"}}</div>
    <h3 class="post-title"><a href="{{url}}" class="link-brand" title="{{{title}}}">{{{title}}}</a></h3>
</li>

{{/foreach}}

</ul>

{{pagination}}
